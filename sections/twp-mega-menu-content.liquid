<script>
  {% comment %} Positioning the Mega Menu {% endcomment %}
  document.addEventListener('DOMContentLoaded', function () {
    const dataAttributeElems = document.querySelectorAll('.twp-mmc-center .twp-mmc-wrapper');
    dataAttributeElems.forEach((elem) => {
      const dataAttribute = elem.getAttribute('data-mega-menu-name');
      const matchingElements = document.querySelectorAll(`.twp-mega_menu_item[data-mega-menu-name="${dataAttribute}"]`);
      matchingElements.forEach((matchingElem) => {
        const rect = matchingElem.getBoundingClientRect();
        matchingElem.addEventListener('mouseover', () => {
          const elemWidth = elem.offsetWidth / 2;
          const positionFromLeft = rect.left - elemWidth;
          elem.closest('.twp-mega-menu-content.twp-mmc-center').style.left = `${positionFromLeft}px`;
        });
      });
    });
  });
</script>

