{% style %}
  {% if section.settings.menu_width == "full-width"%}
    .twp-mega-menu-content {
      width: calc(100% - 40px);
      left: 20px;
    }
  {% endif %}
  {% if section.settings.menu_width =="boxed"%}
    .twp-mega-menu-content {
      width: 1280px;
      left: calc((100% - 1280px) / 2);
    }
  {% endif %}

  .twp-mega-menu-content {
    background-color: {{ section.settings.mmc_bg_color }};
    color: {{ section.settings.mmc_text_color }};
  }

  .twp-mega-menu-content .twp-mmc-sm {
    background-color: {{ section.settings.mmc_bg_color }};
    color: {{ section.settings.mmc_text_color }};
  }

  .twp-mega-menu-content .twp-mmc-sm_2 {
    background-color: {{ section.settings.mmc_bg_color }};
    color: {{ section.settings.mmc_text_color }};
  }

  .twp-mega-menu-content .twp-mmc-sm_3 {
    background-color: {{ section.settings.mmc_bg_color }};
    color: {{ section.settings.mmc_text_color }};
  }

  .twp-mmc-menu_item a{
    color: {{ section.settings.mmc_text_color }};
  }

  .twp-mobile_menu_dropdown .twp-nav_item a{
    color: {{ section.settings.mmc_text_color }};
  }

  .twp-mobile_menu_dropdown .twp-nav_item:not(:last-child)::after {
    content: "";
    position: absolute;
    bottom: 0;
    left: 0;
    background-color: {{ section.settings.mmc_text_color }};
    width: 100%;
    height: 0.1px;
    opacity: 0.3;
  }

  .twp-child_menu_item:not(:last-child)::after {
    content: "";
    position: absolute;
    bottom: 0;
    left: 0;
    background-color: {{ section.settings.mmc_text_color }};
    width: 100%;
    height: 0.1px;
    opacity: 0.3;
  }

  .twp-mobile_menu_dropdown{
    background-color: {{ section.settings.mmc_bg_color }};
  }

  .nav-item-dropdown-open svg path , .nav-item-dropdown-close svg path {
    fill: {{ section.settings.mmc_text_color }};
  }
  .nav-item-child-dropdown-open svg path , .nav-item-child-dropdown-close svg path {
    fill: {{ section.settings.mmc_text_color }};
  }

{% endstyle %}

